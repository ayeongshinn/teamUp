<script>
	
	// 공통 시작
	// 공통 포매팅용
	function formatDate(value) {
	    if (value.length === 8) {
	        return value.substring(0, 4) + "년 " + value.substring(4, 6) + "월 " + value.substring(6, 8) + "일";
	    }
	}
	
	function formatTelNo(value) {
	    if (value.length === 10) {
	        return value.substring(0, 3) + "-" + value.substring(3, 6) + "-" + value.substring(6, 10);
	    } else if (value.length === 9) {
	        return value.substring(0, 2) + "-" + value.substring(2, 5) + "-" + value.substring(5, 9);
	    } else if (value.length === 11) {
	    	return value.substring(0, 3) + "-" + value.substring(3, 7) + "-" + value.substring(7, 11);
	    }	
    }
	
	function formatDateCtrt(value) {
	    if (value.length === 8) {
	        return value.substring(0, 4) + "." + value.substring(4, 6) + "." + value.substring(6, 8);
	    }
	}
	
	// 'YYYY-MM-DD' 형식의 날짜를 'YYYYMMDD'로 변환하는 함수
	function convertToYYYYMMDD(value) {
	    const dateParts = value.split("-");
	    if (dateParts.length === 3) {
	        return dateParts[0] + dateParts[1] + dateParts[2]; // YYYYMMDD 형식으로 변환
	    }
	    return value; // 변환 실패 시 원본 값 반환
	}
	
	// 공통 디스플레이용
	function updateDateDisplay(value) {
	    const yyyymmdd = convertToYYYYMMDD(value); // YYYYMMDD 형식으로 변환
	    document.getElementById('formattedDateDisplay').textContent = formatDate(yyyymmdd); // YYYY년 MM월 DD일로 디스플레이
	}
	
	function nameDisplay(value) {
		document.getElementById('formatNameDisplay').textContent = value;
	}
	
	function telnoDisplay(value) {
		document.getElementById('formatTelnoDisplay').textContent = value;
		document.getElementById('formatTelnoDisplay').textContent = formatTelNo(value);
	}
	
	function ctrtCnclsYmdDisplay(value) {
	    const yyyymmdd = convertToYYYYMMDD(value); // YYYYMMDD 형식으로 변환
	    document.getElementById('formatCtrtCnclsYmdDisplay').textContent = formatDateCtrt(yyyymmdd); // YYYY.MM.DD 형식으로 디스플레이
	}
	// 공통 끝
	
</script>



